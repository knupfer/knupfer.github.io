registerDoMC(8)
calculationtime <- proc.time()
komus = read.table("data/data_komus_bin2.dat",header=TRUE) 
Entropie = NULL
chances = NULL
fitting = NULL
modell = NULL
summe = data.frame()
############
#items = length(komus)
#persons = length(komus[,1])
items = 10
persons = 3
############

EEE = data.frame(matrix(ncol = 1, nrow = items))
SumSD = data.frame(matrix(ncol = 1, nrow = items))
names(EEE) = 'kill'
names(SumSD) = 'kill'
